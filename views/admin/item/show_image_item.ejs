<div
  class="tab-pane fade show active"
  id="image"
  role="tabpanel"
  aria-labelledby="image-tab"
>
  <!-- DataTales Example -->
  <div class="card shadow mb-4">
    <div class="card-body">
      <div class="row">
        <% item.imageId.forEach((res, line) => { %>
        <div class="col-md-4">
          <img
            src="/<%= res.imageUrl %>"
            alt="image"
            style="
              width: 100%;
              display: block;
              border-radius: 0.5rem;
              box-shadow: 0.5rem 0.5rem;
            "
          />
        </div>
        <% }) %>
      </div>
    </div>
  </div>
  <a href="/admin/item" class="btn btn-info btn-circle btn-sm"
    ><i class="fas fa-arrow-left"></i
  ></a>
</div>
